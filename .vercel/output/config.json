{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/posts\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/categories\\/([^/]+?)\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/categories\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/tags\\/([^/]+?)\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/tags\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/$",
			"dest": "render"
		},
		{
			"src": "/.*",
			"dest": "render",
			"status": 404
		}
	]
}